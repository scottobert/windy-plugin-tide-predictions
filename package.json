{
    "name": "windy-plugin-tide-predictions",
    "version": "0.1.0",
    "description": "NOAA tide predictions with current conditions (water/air temp, water level, currents, wind) for stations in view.",
    "main": "dist/plugin.min.js",
    "type": "module",
    "scripts": {
        "build": "rm -rf dist && mkdir dist && SERVE=false rollup -c && cp package.json dist/",
        "build:win": "rmdir /s /q dist && mkdir dist && set SERVE=false && rollup -c && copy package.json dist\\",
        "start": "rollup -w -c",
        "example01": "CONFIG=example01 rollup -w -c",
        "example02": "CONFIG=example02 rollup -w -c",
        "example03": "CONFIG=example03 rollup -w -c",
        "example04": "CONFIG=example04 rollup -w -c",
        "example05": "CONFIG=example05 rollup -w -c",
        "example06": "CONFIG=example06 rollup -w -c",
        "example07": "CONFIG=example07 rollup -w -c",
        "build:all-examples": "for number in 01 02 03 04 05 06; do CONFIG=example$number SERVE=false rollup -c; done",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/scottobert/windy-plugin-tide-predictions.git"
    },
    "keywords": [
        "windy",
        "windy.com",
        "plugins",
        "tides",
        "noaa",
        "currents",
        "weather"
    ],
    "author": "Scott Obert",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/scottobert/windy-plugin-tide-predictions/issues"
    },
    "homepage": "https://github.com/scottobert/windy-plugin-tide-predictions#readme",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@windycom/plugin-devtools": "^3.0.2",
        "d3": "^7.9.0"
    },
    "devDependencies": {
        "@testing-library/dom": "^10.4.1",
        "@testing-library/svelte": "^5.2.9",
        "@vitest/coverage-v8": "^4.0.15",
        "@vitest/ui": "^4.0.15",
        "jsdom": "^27.3.0",
        "vitest": "^4.0.15"
    }
}
